{
    "manifest_version": 2,
    "name": "ChromeExtentionPair8",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["app.js"], 
            "css": ["style.css"],
            "html": ["index.html"]
        }
    ],
    "browser_action": {
        "default_popup": "index.html",
        "default_title": "Make It Better"
    }
}



