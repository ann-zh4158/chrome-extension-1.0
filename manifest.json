{
    "manifest_version": 2,
    "name": "Visual Mad Libs",
    "version": "1.0.0",
    "permissions": ["activeTab", "<all_urls>", "tabs"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["app.js"]
        }
    ],
    "browser_action": {
        "default_icon": "cat.png",
        "default_popup": "index.html",
        "default_title": "Visual Mad Libs"
    }
}

